{
  "compilerOptions": {
    "target": "ES2020",                          
    "module": "ESNext",                    
    "lib": ["es6"],                     
    "allowJs": true,
    "outDir": "build",                          
    "rootDir": "src",
    "strict": true,         
    "noImplicitAny": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "types": ["node"]
  },
  "include": ["src/**/*", "src/notion"]
}